{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "comite-clasificacion-joffroy": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/comite-clasificacion-joffroy",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/plugins/global/plugins.bundle.css",
              "src/styles.css",
              "src/assets/css/animated/animated.min.css",
              "src/assets/css/custom.css",
              "src/assets/plugins/custom/datatables/datatables.bundle.css",
              "node_modules/lightbox2/src/css/lightbox.css"
            ],
            "scripts": [
              "src/assets/plugins/global/plugins.bundle.js",
              "src/scripts.bundle.js",
              "src/assets/plugins/custom/datatables/datatables.bundle.js",
              "src/assets/plugins/custom/fslightbox/fslightbox.bundle.js",
              "node_modules/lightbox2/src/js/lightbox.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "10mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "comite-clasificacion-joffroy:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "comite-clasificacion-joffroy:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "comite-clasificacion-joffroy:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "comite-clasificacion-joffroy:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "comite-clasificacion-joffroy:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "comite-clasificacion-joffroy"
}
